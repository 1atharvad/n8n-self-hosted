{
  "name": "n8n-deploy",
  "version": "1.0.0",
  "description": "Self deployment for n8n for AD",
  "keywords": [
    "n8n",
    "workflows"
  ],
  "license": "ISC",
  "author": "Atharva Devasthali",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests",
    "build-n8n-nodes": "npx rimraf ./n8n-data/custom && tsc && gulp",
    "pip-install": "pip3.11 install -r requirements.txt",
    "add-pkg": "sh -c 'pip3.11 install \"$0\" && echo \"$(pip3.11 freeze | grep ^$0==)\" >> requirements.txt'",
    "docker-build": "sh api/build.sh",
    "docker-push-build": "sh api/build.sh --push",
    "docker-up:dev": "docker-compose -f docker-compose.dev.yml up --build -d",
    "docker-up:prod": "docker-compose -f docker-compose.prod.yml up --build -d",
    "docker-down:dev": "docker-compose -f docker-compose.dev.yml down",
    "docker-down:prod": "docker-compose -f docker-compose.prod.yml down",
    "docker-rm-cache": "docker container prune -f && docker image prune -a -f && docker volume prune -f && docker builder prune -a -f",
    "docker-update-images": "docker pull n8nio/n8n && docker pull ghcr.io/1atharvad/mediasynth && npm run docker-down && npm run docker-up",
    "push-workflows": "sh sh_files/backup_n8n_workflows.sh ./n8n-workflow",
    "push-workflows-backup": "sh sh_files/git_n8n_workflows.sh",
    "pull-workflows-backup": "sh sh_files/upload_n8n_workflows.sh"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "gulp": "^5.0.0",
    "jest": "^30.1.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
